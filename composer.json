{
    "name": "eloquent/phpstan-phony",
    "description": "PHPStan configuration for Phony.",
    "keywords": ["mock", "mocking", "stub", "stubbing", "spy", "dummy", "double", "test", "fake", "static", "analysis"],
    "homepage": "http://eloquent-software.com/phony/",
    "license": "MIT",
    "authors": [
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        }
    ],
    "type": "phpstan-extension",
    "require": {
        "php": ">=7.2",
        "phpstan/phpstan": "^0.12-dev"
    },
    "require-dev": {
        "eloquent/code-style": "^1",
        "eloquent/phony": "^3",
        "eloquent/phony-kahlan": "^2",
        "eloquent/phony-phpunit": "^4 || ^5",
        "friendsofphp/php-cs-fixer": "^2",
        "kahlan/kahlan": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Eloquent\\Phpstan\\Phony\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eloquent\\Phpstan\\Phony\\Test\\": "test/src"
        },
        "files": [
            "test/src/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8.x-dev"
        },
        "phpstan": {
            "includes": [
                "phony.neon"
            ]
        }
    }
}
